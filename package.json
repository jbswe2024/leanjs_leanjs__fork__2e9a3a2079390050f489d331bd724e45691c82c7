{
  "name": "@leanjs/root",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/shell",
    "examples/micros/*",
    "examples/packages/*"
  ],
  "scripts": {
    "run:examples": "concurrently \"yarn run:shell\" \"yarn run:profile\" \"yarn run:todo\" \"yarn run:feed\"",
    "run:shell": "cd examples/shell && yarn start",
    "run:profile": "cd examples/micros/profile && yarn start",
    "run:feed": "cd examples/micros/feed && yarn start",
    "run:todo": "cd examples/micros/todo && yarn start"
  },
  "devDependencies": {
    "@swc/core": "^1.2.142",
    "@swc/jest": "^0.2.17",
    "@types/jest": "^27.4.0",
    "concurrently": "^7.0.0",
    "del-cli": "^4.0.1",
    "jest": "^27.5.1",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.17"
}
